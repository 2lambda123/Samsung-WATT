<!-- ADD THE CLASS layout-top-nav TO REMOVE THE SIDEBAR. -->
<body class="hold-transition skin-blue layout-top-nav">
<div class="wrapper">

<header class="main-header">
  <nav class="navbar navbar-static-top">
    <div class="container">
      <div class="navbar-header">
        <a href="/project" class="navbar-brand"><b>WATT</b></a>
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
          <i class="fa fa-bars"></i>
        </button>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
        <ul class="nav navbar-nav">
          <!--li class="active"><a href="#">Link <span class="sr-only">(current)</span></a></li-->
          <li><a href="/project">Projects</a></li>
        </ul>
      </div>

      <div class="navbar-brand pull-right btn-group">
        <div class="btn btn-xs btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <%= user.local.email %> <span class="caret"></span>
        </div>
        <ul class="dropdown-menu">
          <li><a href="/logout">Logout</a></li>
        </ul>
      </div>
      <!-- /.navbar-collapse -->
    </div>
    <!-- /.container-fluid -->
  </nav>
</header>
<!-- Full Width Column -->
<div class="content-wrapper">
  <div class="container">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <ul class="nav nav-pills">
        <li>
          <h3>
            <i class="fa fa-book"></i> Projects
            <!--small>Example 2.0</small-->
          </h3>
        </li>
        <li class="pull-right">
          <a class="btn btn-app" data-toggle="modal" data-target="#import-project" data-backdrop="false">
            <i class="fa fa-download"></i>Import Project
          </a>
          <a class="btn btn-app" data-toggle="modal" data-target="#new-project" data-backdrop="false">
            <i class="fa fa-plus"></i>New Project
          </a>
        </li>
      </ul>
    </section>
    <% include modals/deleteproject %>
    <% include modals/editproject %>
    <% include modals/exportproject %>
    <% include modals/importproject %>
    <% include modals/newproject %>
    <% include modals/shareproject %>
    <!-- Main content -->
    <section class="content">
      <div id="project-box" class="box box-primary">
        <div class="box-body">
          <ul id="project-list" class="margin products-list product-list-in-box">
            <% include parts/projectlist %>
          </ul>
        </div>
      </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.container -->
</div>
<!-- /.content-wrapper -->
<!-- <footer class="main-footer">
</footer> -->
</div>
</body>
